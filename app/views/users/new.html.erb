<h2>sing in</h2>
<%= form_for :user, url: users_path do |f|%>
<p>
login <br>
<%= f.text_field :name %>
</p>
<p>
password <br>
<%= f.password_field :password %>
</p>
<p>
confirm password <br>
<%= f.password_field :confirmPassword %>
</p>
<p>
number <br>
<%= f.text_field :accountNumber %>
</p>
<p>
<%= f.submit %>
</p>
<% end %>